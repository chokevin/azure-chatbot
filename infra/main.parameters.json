{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "microsoftAppId": {
      "value": "${BOT_APP_ID:=}"
    },
    "microsoftAppPassword": {
      "value": "${BOT_APP_PASSWORD:=}"
    },
    "microsoftAppTenantId": {
      "value": "${BOT_APP_TENANT_ID:=}"
    },
    "microsoftAppType": {
      "value": "${BOT_APP_TYPE:=UserAssignedMSI}"
    },
    "botDisplayName": {
      "value": "${BOT_DISPLAY_NAME:=Azure Bot Sample for Teams}"
    },
    "botDescription": {
      "value": "${BOT_DESCRIPTION:=Echo bot with Teams integration built with .NET}"
    },
    "appServicePlanSku": {
      "value": "${APP_SERVICE_PLAN_SKU:=F1}"
    }
  }
}
